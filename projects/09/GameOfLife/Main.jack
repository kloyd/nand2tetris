// File name: projects/09/Life/Main.jack

/** Game of Life program. */
class Main {
	function void main() {
		// Initialize a game
		var LifeGame lifeGame;
		let lifeGame = LifeGame.new();

		// Run the game
		do lifeGame.run();

		// End the game 
		do lifeGame.dispose();
		do Screen.clearScreen();
		do Output.moveCursor(11, 27);
		do Output.printString("GAME OVER");

		return;
		}
	}

